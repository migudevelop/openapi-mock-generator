{
  "name": "openapi-mock-generator",
  "version": "0.0.1",
  "description": "CLI tool to generate mock data from OpenAPI specifications.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:prod": "node ./dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "bin": {
    "openapi-mock-generator": "./dist/bin.js"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.MD",
    "package.json"
  ],
  "keywords": [
    "openapi-mock-generator",
    "openapi-mock",
    "openapi-tools",
    "openapi-generator",
    "openapi",
    "mock",
    "generator",
    "cli",
    "typescript",
    "mocks",
    "api",
    "swagger",
    "json",
    "schema",
    "faker",
    "javascript"
  ],
  "author": "Miguel MartÃ­nez",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22.0.x",
    "pnpm": ">=9.0.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/migudevelop/openapi-mock-generator.git"
  },
  "homepage": "https://github.com/migudevelop/openapi-mock-generator.git",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.1.1",
    "@faker-js/faker": "9.7.0",
    "@migudevelop/types-utils": "1.3.0",
    "colorette": "2.0.20",
    "cosmiconfig": "9.0.0",
    "express": "5.1.0",
    "json-schema-faker": "0.5.9",
    "openapi-types": "12.1.3",
    "tsconfig-paths": "4.2.0"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.1",
    "jest": "29.7.0",
    "ts-jest": "29.3.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3"
  }
}
